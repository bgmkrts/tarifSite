<%= render 'shared/page_title', title: "Create a new Style" %>

<%= render 'shared/errors', obj: @style %>

<div class="row">
    <div class="well col-md-8 col-md-offset-2">
        <%= form_for @style do |f| %>
            <%= f.label :Yeni_Mutfak_Adı %>
            <%= f.text_field :name %>
            
            
            <%= f.submit 'Mutfağı Oluştur', class: "btn btn-success" %>
        <% end %>          
    </div>
</div>